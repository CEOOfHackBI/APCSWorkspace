/**
 * 
 */

/**
 * @author jack
 *
 */
public class MyQwewe implements Qwewe{

	/**
	 * @param args
	 */
	
	private Object[] qwewe;
	private int front=0;
	private int back=0;
	
	public MyQwewe(int capacity)
	{
		qwewe=new Object[capacity];
	}
	
	
	public boolean add(Object obj)
	{
		if(front==back&&qwewe[front]!=null)
			return false;
		
		qwewe[front]=obj;
		if(front!=qwewe.length-1)
			front++;
		else
			front=0;
		
		return true;
		
	}
	
	public boolean isEmpty()
	{
		return front==back;
	}
	
	public Object remove()
	{
		if(front==qwewe.length-1)
		{
			front=0;
			Object o=qwewe[qwewe.length-1];
			qwewe[qwewe.length-1]=null;
			return o;
		}
		else
		{
			front++;
			return qwewe[front-1];
		}
	}
	
	public Object peek()
	{
		return qwewe[front];
	}
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
