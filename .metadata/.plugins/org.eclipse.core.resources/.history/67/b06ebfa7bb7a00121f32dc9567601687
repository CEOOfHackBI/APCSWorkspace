/**
 * 
 */

/**
 * @author jack
 *
 */
public class MyQwewe implements Qwewe{

	/**
	 * @param args
	 */
	
	private Object[] qwewe;
	private int front=0;
	private int back=0;
	
	public MyQwewe(int capacity)
	{
		qwewe=new Object[capactiy];
	}
	
	
	public boolean add(Object obj)
	{
		if(qwewe.length!=0&&front==back)
			return false;
		
		qwewe[front]=obj;
		if(front!=9)
			front++;
		else
			front=0;
		
		return true;
		
	}
	
	public boolean isEmpty()
	{
		if(front==back)
			return false;
		return true;
	}
	
	public Object remove()
	{
		if(front==9)
		{
			front=0;
			return qwewe[9];
		}
		else
		{
			front++;
			return qwewe[front-1];
		}
	}
	
	public Object peek()
	{
		return qwewe[front];
	}
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
