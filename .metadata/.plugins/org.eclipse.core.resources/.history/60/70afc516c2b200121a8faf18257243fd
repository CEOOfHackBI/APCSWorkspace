public class Trail {

	/**
	 * @param args
	 */
	
	private int[] markers;
	
	public boolean isLevelTrailSegment(int start, int end)
	{
		int max=start;
		int min=start;
		
		
		return Math.abs(max-min)<=10;
	}
	
	
