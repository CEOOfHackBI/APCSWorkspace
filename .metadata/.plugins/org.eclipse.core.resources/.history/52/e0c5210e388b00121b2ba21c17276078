 */
public class ClassVersion {

	/**
	 * @param args
	 */
	
	
	private static int getDigit(int number, int k)
	{
		k+=1;
		int mod=1;
		
		for(int i=0;i<k-1;i++)
		{
			mod*=10;
		}
		
		number/=(mod);
		return number%10;
	}
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
